    "data=pd.read_csv('C://data.csv')\n",
    "data.drop('Hours', axis=1 , inplace=True )\n",
    "x=data.drop('Battery Energy (Mwh)', axis=1,)\n",
    "y=data['Battery Energy (Mwh)']\n",
    "x_train, x_test , y_train , y_test= train_test_split(x, y, test_size=0.3, random_state=42)\n",
    "reg=LinearRegression()\n",
    "reg.fit(x_train, y_train)\n",
    "y_pred=reg.predict(x_test)\n",
    "plt.figure(figsize=(28,13))\n",
    "plt.rcParams.update({'font.size': 12})\n",
    "plt.scatter(y_test, y_pred, s=200)\n",
    "plt.title('Battery storage enregy in each hour (Mwh)', fontsize=30)\n",
    "plt.xticks([-0.12, -0.1, 0.076, 0.22, 0.28, 0.29, 0.34, 0.37, 0.455, 0.48, 0.640, 0.70])\n",
    "plt.yticks([-0.19045, -0.11, 0.17, 0.288, 0.304, 0.345, 0.411, 0.476, 0.518, 0.586, 0.707, 0.740])\n",
    "plt.xlabel('Battery Energy', fontsize=25)\n",
    "plt.ylabel('Predicted Battery Energy', fontsize=25)\n",
    "plt.show()\n",
    "\n",
    "a=mean_squared_error(y_test, y_pred)\n",
    "print(*['MSE is =', a,])\n",
    "\n",
    "y_pred_reshaped=y_pred.reshape(-1 , 1)\n",
    "Result_df=pd.DataFrame(y_test)\n",
    "Result_df['Predicted Battert Energy (Mwh)'] = y_pred_reshaped\n",
    "Result_df\n"
   ]
